{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|images|html|favicon\\.ico)/(.*)",
      "dest": "src/public/$1/$2"
    },
    {
      "src": "/favicon.ico",
      "dest": "src/public/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
